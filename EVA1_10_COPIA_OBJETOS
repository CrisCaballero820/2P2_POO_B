package eva1_10_copia_objetos;

public class eva1_10_copia_objetos {
    public static void main(String[] args){
        Prueba obj = new Prueba();
        System.out.println("Direccion: " + obj);
        obj.x = 100;
        System.out.println("Valor de x = " + obj.x);
    
    }
    
}
class Prueba{
    public int x;

}
